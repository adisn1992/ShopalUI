<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adimarsiano.shopalui">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".signIn_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".stock_Activity"></activity>
        <activity android:name=".BarcodeScannerActivity"></activity>
        <activity android:name=".details_Account_Activity"></activity>
        <activity android:name=".shoppingList_Activity"></activity>
        <activity android:name=".home_Activity"></activity>
    </application>
</manifest>